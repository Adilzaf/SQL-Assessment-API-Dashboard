    <DataSet Name="DS_LastAssessments">
      <Query>
        <DataSourceName>Assessment_DB</DataSourceName>
        <CommandText>  Select h.*,Count(d.Assessment_ID) as Issues_Number  From [Assessment].[Results_Header] h
  inner join [Assessment].[Results_History] d on h.Assessment_ID = d.Assessment_ID
  Group by h.[Assessment_ID],h.[Assessment_Type],h.[Assessment_Target],h.[Assessment_Date]
  having   h.[Assessment_ID] IN
  ( Select Max([Assessment_ID])
  FROM [Assessment].[Results_Header]
  group by [Assessment_Type],[Assessment_Target])</CommandText>
      </Query>
